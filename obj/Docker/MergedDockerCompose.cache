    - /Users/belakovacs/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
  productapi:
    build:
      context: /Volumes/T7 Touch/suli/online_retailer_microservices_assignment
      dockerfile: ProductApi/Dockerfile
