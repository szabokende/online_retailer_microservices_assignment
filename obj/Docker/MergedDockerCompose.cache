    build:
      context: /Volumes/T7 Touch/suli/online_retailer_microservices_assignment
      dockerfile: OrderApi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: orderapi
    ports:
    - published: 8080
      target: 80
    - published: 8443
